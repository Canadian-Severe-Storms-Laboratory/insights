{
    "name": "insights",
    "type": "module",
    "private": true,
    "scripts": {
        "build": "turbo check-types lint build",
        "dev": "turbo dev --filter '!@insights/worker-point-cloud' --filter=!./packages/*",
        "lint": "turbo lint",
        "check-types": "turbo check-types",
        "prettier:check": "prettier --check .",
        "prettier:write": "prettier --write ."
    },
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "devDependencies": {
        "@types/bun": "latest",
        "oxlint": "^1.29.0",
        "oxlint-tsgolint": "^0.8.1",
        "prettier": "^3.6.2",
        "prettier-plugin-organize-imports": "^4.3.0",
        "prettier-plugin-tailwindcss": "^0.7.1",
        "turbo": "^2.7.2"
    },
    "peerDependencies": {
        "typescript": "^5.9.3"
    },
    "packageManager": "bun@1.3.5"
}
