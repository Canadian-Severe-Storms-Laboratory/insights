{
    "$schema": "https://turborepo.com/schema.json",
    "tasks": {
        "build": {
            "dependsOn": ["^build", "check-types", "lint"],
            "outputs": ["dist/**"]
        },
        "build-types": {
            "outputs": []
        },
        "check-types": {
            "dependsOn": ["^build-types"],
            "outputs": []
        },
        "lint": {
            "outputs": [],
            "inputs": ["$TURBO_DEFAULT$", ".oxlintrc.json"]
        },
        "dev": {
            "persistent": true,
            "cache": false,
            "inputs": ["$TURBO_DEFAULT$", ".env"]
        }
    }
}
