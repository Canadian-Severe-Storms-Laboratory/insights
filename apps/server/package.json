{
    "name": "@insights/server",
    "main": "src/index.ts",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "dev": "bun run --env-file=../../.env --hot src/index.ts",
        "build": "bun build src/index.ts --external sharp --outdir dist --target bun",
        "build-types": "tsc",
        "start": "bun run dist/index.js",
        "clean": "rm -rf dist",
        "check-types": "tsc --noEmit",
        "lint": "oxlint -c ../../.oxlintrc.json 'src/**/*.ts' 'src/**/*.tsx'"
    },
    "dependencies": {
        "@hono/zod-validator": "^0.7.4",
        "@insights/shared": "workspace:*",
        "@xmldom/xmldom": "^0.8.11",
        "better-auth": "^1.3.34",
        "bullmq": "^5.64.1",
        "drizzle-orm": "^0.44.7",
        "exifreader": "^4.32.0",
        "hono": "^4.10.6",
        "ioredis": "^5.8.2",
        "sharp": "^0.34.5",
        "zod": "^4.1.12"
    },
    "devDependencies": {
        "@types/bun": "latest",
        "pg": "^8.16.3"
    }
}
